{
    "org": "$HZN_ORG_ID",
    "label": "$SERVICE_NAME for $ARCH",
    "description": "Open Horizon service for the open-source Fledge project software",
    "public": true,
    "documentation": "https://github.com/open-horizon-services/service-fledge",
    "url": "$SERVICE_NAME",
    "version": "$SERVICE_VERSION",
    "arch": "$ARCH",
    "sharable": "singleton",
    "requiredServices": [],
    "userInput": [],
    "deployment": {
        "services": {
            "$SERVICE_NAME": {
                "image": "${DOCKER_IMAGE_BASE}_$ARCH:$SERVICE_VERSION",
                "privileged": false,
                "ports": [
                    {"HostIP":"0.0.0.0","HostPort":"8081:8081/tcp"},
                    {"HostIP":"0.0.0.0","HostPort":"1995:1995/tcp"},
                    {"HostIP":"0.0.0.0","HostPort":"8082:80/tcp"}
                ]
            }
        }
    }
}